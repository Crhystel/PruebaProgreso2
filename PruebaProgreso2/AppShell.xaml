<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PruebaProgreso2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PruebaProgreso2"
    Shell.FlyoutBehavior="Disabled"
    Title="PruebaProgreso2">

    <TabBar>
        <Tab Title="Recarga">
            <ShellContent
                Title="lo"
                ContentTemplate="{DataTemplate local:VelascoCRecarga}"
                Route="VelascoCRecarga"/>
        </Tab>
        <Tab Title="Grid">
            <ShellContent
                 Title="ww"
                 ContentTemplate="{DataTemplate local:VelascoCGrid}"
                 Route="VelascoCGrid"/>
        </Tab>

    </TabBar>

</Shell>
